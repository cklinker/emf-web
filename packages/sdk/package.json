{
  "name": "@emf/sdk",
  "version": "0.0.1",
  "description": "Type-safe TypeScript client for EMF APIs",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "emf-generate-types": "./dist/cli/generate-types.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli/index.js",
      "require": "./dist/cli/index.cjs",
      "types": "./dist/cli/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "generate-types": "node --loader ts-node/esm src/cli/generate-types.ts",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "axios": "^1.6.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "axios": "^1.6.7",
    "zod": "^3.22.4"
  },
  "keywords": [
    "emf",
    "sdk",
    "typescript",
    "api-client",
    "rest-client",
    "openapi",
    "type-generation"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/emf/emf-web.git",
    "directory": "packages/sdk"
  },
  "homepage": "https://github.com/emf/emf-web/tree/main/packages/sdk#readme",
  "bugs": {
    "url": "https://github.com/emf/emf-web/issues"
  },
  "author": "EMF Team",
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false
}
